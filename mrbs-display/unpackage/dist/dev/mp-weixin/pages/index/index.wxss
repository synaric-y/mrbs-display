
.container {
		width: 750rpx;
		height: 100vh;
		display: flex;
		flex-direction: row;
		justify-content: center;
		align-items: center;
		position: relative;
}
.container-inner {
		width: 750rpx;
		height: 468rpx;
		position: relative;
		box-sizing: border-box;
}
.bg {
		width: 750rpx;
		height: 100vh;
		position: absolute;
		top: 0;
		left: 0;
}
.now-time {
		position: absolute;
		left: 46rpx;
		top: 41rpx;
		height: 68rpx;
		line-height: 68rpx;
		font-size: 48rpx;
		color: white;
}
.now-time-tail {
		font-size: 28rpx;
		color: white;
		margin-left: -8rpx;
}
.now-date {
		position: absolute;
		left: 46rpx;
		top: 93rpx;
		height: 43rpx;
		line-height: 43rpx;
		font-size: 19rpx;
		color: white;
}
.divider-one {
		position: absolute;
		left: 0;
		top: 150rpx;
		width: 359rpx;
		height: 1rpx;
		background-color: rgba(255, 255, 255, 0.1);
}
.room-icon-cir {
		position: absolute;
		top: 46rpx;
		right: 273rpx;
		width: 127rpx;
		height: 127rpx;
}
.room-icon {
		position: absolute;
		top: 43rpx;
		left: 72rpx;
		right: 0;
		height: 127rpx;
		line-height: 127rpx;
		text-align: center;
		font-size: 92rpx;
		color: white;
		font-weight: bold;
		text-shadow: 0px 2px 6px rgba(0, 0, 0, 0.4)
}
.room-tail {
		position: absolute;
		top: 71rpx;
		right: 11rpx;
		width: 252rpx;
		height: 102rpx;
		line-height: 102rpx;
		font-size: 70rpx;
		color: white;
		text-shadow: 0px 2px 6px rgba(0, 0, 0, 0.4)
}
.content-container {
		position: absolute;
		top: 150rpx;
		left: 0;
		width: 750rpx;
		height: 191rpx;
		display: flex;
		flex-direction: row;
		align-items: center;
		justify-content: center;
}
.room-status {
		width: 393rpx;
		height: 168rpx;
		line-height: 168rpx;
		font-size: 100rpx;
		text-shadow: 0px 2px 6px rgba(0, 0, 0, 0.4);
		margin-top: 42rpx;
}
.room-status-using {
		color: #FFFF00;
}
.room-status-idle {
		color: #00FF02;
}
.room-content-container {
		flex: 1;
		display: flex;
		flex-direction: column;
		align-items: flex-start;
}
.room-space {
		width: 46rpx;
		height: 100rpx;
}
.room-name {
		height: 43rpx;
		margin-top: 14rpx;
		line-height: 43rpx;
		font-size: 18rpx;
		color: white;
}
.room-entry-duration {
		height: 47rpx;
		margin-top: 11rpx;
		line-height: 47rpx;
		font-size: 48rpx;
		color: white;
		text-shadow: 0px 2px 6px rgba(0, 0, 0, 0.4)
}
.room-booker {
		height: 43rpx;
		margin-top: 11rpx;
		line-height: 43rpx;
		font-size: 18rpx;
		color: white;
}
.divider-two {
		position: absolute;
		left: 0;
		top: 342rpx;
		width: 750rpx;
		height: 1rpx;
		background-color: rgba(255, 255, 255, 0.1);
}
.timeline-container {
		position: absolute;
		left: 37rpx;
		right: 37rpx;
		top: 364rpx;
}
.time-span-container {
		width: 100%;
		height: 30rpx;
		display: flex;
		flex-direction: row;
		align-items: center;
}
.time-span-text {
		font-size: 12rpx;
		color: white;
		flex: 1;
		text-align: center;
}
.time-span-dot {
		font-size: 12rpx;
		color: white;
}
.timeline-line-container {
		width: 100%;
		height: 22rpx;
		display: flex;
		flex-direction: row;
		align-items: center;
}
.timeline-line-item {
		text-align: center;
		line-height: 22rpx;
		font-size: 12rpx;
		color: white;
}
.logo-container {
		width: 100%;
		height: 14rpx;
		display: flex;
		justify-content: center;
		align-items: center;
		margin-top: 14rpx;
}
.logo {
		width: 177rpx;
		height: 13rpx;
}
